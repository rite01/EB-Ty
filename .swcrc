{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "dynamicImport": true,
        "decorators": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2015",
      "externalHelpers": false,
      "keepClassNames": true,
      "loose": false,
      "minify": {
        "compress": false,
        "mangle": false
      },
      "baseUrl": "src",
      "paths": {
        "@/*": ["*"],
        "@app/*": ["*"],
        "@config/*": ["config/*"],
        "@libs/*": ["libs/*"],
        "@constants/*": ["constants/*"],
        "@controllers/*": ["controllers/*"],
        "@databases/*": ["databases/*"],
        "@interfaces/*": ["interfaces/*"],
        "@models/*": ["models/*"],
        "@routes/*": ["routes/*"],
        "@services/*": ["services/*"],
        "@utils/*": ["utils/*"]
      }
    },
    "module": {
      "type": "commonjs"
    }
  }
  